{
  "name": "@reference-ui/system",
  "version": "0.0.1",
  "description": "Reference UI - Panda CSS design system wrapper",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./css": {
      "types": "./styled-system/css/index.d.ts",
      "import": "./styled-system/css/index.mjs"
    },
    "./recipes": {
      "types": "./styled-system/recipes/index.d.ts",
      "import": "./styled-system/recipes/index.mjs"
    },
    "./patterns": {
      "types": "./styled-system/patterns/index.d.ts",
      "import": "./styled-system/patterns/index.mjs"
    },
    "./tokens": {
      "types": "./styled-system/tokens/index.d.ts",
      "import": "./styled-system/tokens/index.mjs"
    },
    "./panda.css": "./panda.css"
  },
  "files": [
    "dist",
    "styled-system",
    "panda.css"
  ],
  "scripts": {
    "build": "panda codegen && panda cssgen --outfile=panda.css && tsc",
    "dev": "panda --watch",
    "codegen": "panda codegen"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.45.0"
  },
  "license": "MIT"
}
