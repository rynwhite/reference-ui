{
  "name": "@reference-ui/gen",
  "version": "0.0.1",
  "private": true,
  "description": "Reference design system compiler - consumes core, produces BuildResult in memory",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "@reference-ui/core": "workspace:*",
    "@pandacss/dev": "^0.49.0"
  },
  "devDependencies": {
    "typescript": "~5.9.2",
    "chokidar": "^4.0.0",
    "tsdown": "^0.20.1"
  }
}
