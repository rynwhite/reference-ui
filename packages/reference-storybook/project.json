{
  "name": "reference-storybook",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/reference-storybook/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/reference-storybook",
        "main": "packages/reference-storybook/src/index.ts",
        "tsConfig": "packages/reference-storybook/tsconfig.lib.json",
        "assets": ["packages/reference-storybook/*.md"]
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "dependsOn": ["reference-core:build"],
      "options": {
        "command": "pnpm run dev",
        "cwd": "packages/reference-storybook"
      }
    }
  },
  "tags": []
}
