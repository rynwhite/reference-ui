{
  "name": "old-reference-storybook",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/old/reference-storybook/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm run build-storybook",
        "cwd": "packages/old/reference-storybook"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "dependsOn": ["old-reference-core:build"],
      "options": {
        "command": "pnpm run dev",
        "cwd": "packages/old/reference-storybook"
      }
    }
  },
  "tags": []
}
