{
  "name": "reference-docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/reference-docs/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dev",
        "cwd": "packages/reference-docs"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "packages/reference-docs"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm preview",
        "cwd": "packages/reference-docs"
      }
    }
  }
}
